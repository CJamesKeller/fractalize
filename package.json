{
  "name": "elasticity",
  "version": "1.0.0",
  "scripts": {
    "dev": "parallelshell 'npm run dev-server' 'npm run dev-client'",
    "dev-server": "nodemon ./bin/www --ignore client --ignore public",
    "dev-client": "watchify --fast --verbose client/index.js -o public/bundle.js",
    "start": "node ./bin/www",
    "build": "browserify client/index.js | uglifyjs > public/bundle.js"
  },
  "browserify": {
    "transform": [
      "brfs",
      [
        "sheetify/transform",
        {
          "use": "sheetify-nested"
        }
      ],
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "bankai": "^8.1.1",
    "brfs": "^1.4.3",
    "browserify": "^14.4.0",
    "css-extract": "^1.2.0",
    "envify": "^4.1.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^3.0.1",
    "sheetify": "^6.1.1",
    "sheetify-nested": "^1.0.2",
    "uglify-js": "^3.0.28",
    "uglifycss": "0.0.27",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "choo": "^6.0.0",
    "elasticsearch": "^13.3.1",
    "express": "^4.15.4",
    "hbs": "^4.0.1"
  },
  "author": "christopher keller"
}
